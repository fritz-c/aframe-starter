{
  "name": "aframe-starter",
  "description": "A-Frame Starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --progress --colors --hot-only --inline --port 1234 --open",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "prettier": "prettier --single-quote --trailing-comma es5 --write '**/*.{js,jsx,css,md}' '!{CHANGELOG.md,**/*.min.js,{dist,coverage}/**/*}'",
    "prod-run": "NODE_ENVIRONMENT=production node server.js",
    "lint": "eslint .",
    "deploy": "ghpages -p .ghpages && rm -rf .ghpages",
    "predeploy": "npm run build && shx mkdir -p .ghpages && cp -r index.html assets dist .ghpages"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "aframe-super-hot-html-loader": "^2.0.0",
    "aframe-super-hot-loader": "^1.6.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "ghpages": "0.0.10",
    "html-require-loader": "^1.0.1",
    "json-loader": "^0.5.7",
    "nunjucks": "^3.2.0",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "style-loader": "^0.23.1",
    "super-nunjucks-loader": "^2.0.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-glsl-loader": "^1.0.1"
  }
}
